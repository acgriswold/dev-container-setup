{
  "name": "postgres-drizzle",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:reset": "pnpm truncate && pnpm migrate && pnpm push && pnpm pull && pnpm seed:generate",
    "seed": "pnpm cli seed",
    "seed:generate": "pnpm seed --generate",
    "migrate": "pnpm migrate:generate && pnpm migrate:run",
    "migrate:generate": "pnpm drizzle-kit generate:pg",
    "migrate:run": "pnpm cli migrate",
    "cli:test": "pnpm cli test",
    "truncate": "pnpm cli truncate",
    "cli": "pnpm tsx --tsconfig ./tsconfig.scripts.json drizzle/commands/cli.ts",
    "studio": "pnpm drizzle-kit studio --port 1337 --verbose",
    "pull": "pnpm drizzle-kit introspect:pg",
    "push": "pnpm drizzle-kit push:pg",
    "drop": "pnpm drizzle-kit drop"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@vercel/postgres": "0.5.0",
    "autoprefixer": "10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.28.6",
    "eslint": "8.51.0",
    "eslint-config-next": "13.5.4",
    "ms": "^2.1.3",
    "next": "13.5.4",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@types/ms": "^0.7.32",
    "@types/node": "20.8.3",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.11",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.19.13",
    "fs": "0.0.1-security",
    "mime": "^4.0.1",
    "minimist": "^1.2.8",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.1"
  }
}
